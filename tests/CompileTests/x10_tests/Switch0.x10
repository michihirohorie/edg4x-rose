class Switch0 {
    def func(var num:Int) {
        var i:Int;
        switch (num) {
            default:
                i = 10n;
                break;
            case 0n:
                i = 0n;
                break;
            case 1n:
                i = 1n;
                break;
            case 2n:
                i = 2n;
                break;
        }
    }
}
